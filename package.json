{
  "name": "effector-storage",
  "description": "Module for Effector to sync stores with different storages",
  "version": "5.0.0",
  "author": "Victor Didenko <yumaa.verdin@gmail.com> (https://yumaa.name)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yumauri/effector-storage"
  },
  "bugs": {
    "url": "https://github.com/yumauri/effector-storage/issues"
  },
  "homepage": "https://github.com/yumauri/effector-storage#readme",
  "keywords": [
    "effector",
    "persist",
    "storage",
    "localstorage",
    "local storage",
    "sessionstorage",
    "session storage",
    "react native asyncstorage",
    "react native async storage",
    "react native encryptedstorage",
    "react native encrypted storage"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup --config",
    "build:dev": "rollup --config",
    "check:lint": "eslint src --ext .ts",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check src",
    "check:spell": "yaspeller .",
    "lint": "npm-run-all --parallel check:*",
    "format": "prettier --write src tests",
    "test": "npm-run-all --parallel test:*",
    "test:node": "nyc --reporter=lcov --reporter=text uvu -r ts-node/register -r tests/rewiremock -i mock -i types tests",
    "test:types": "uvu -r ts-node/register -r tests/rewiremock tests types",
    "doc:update-readme": "npx doctoc --maxlevel 3 README.md",
    "clean": "rm -rf build coverage .nyc_output",
    "size": "size-limit --highlight-less",
    "release": "np --yolo --no-release-draft --no-2fa --no-cleanup --contents ./build",
    "version": "pnpm build"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-block-scoping": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@react-native-async-storage/async-storage": "^1.17.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@size-limit/preset-small-lib": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "effector": "21.0.0",
    "effector-21-0-0": "npm:effector@21.0.0",
    "effector-21-8-12": "npm:effector@21.8.12",
    "effector-22-0-0": "npm:effector@22.0.0",
    "effector-22-3-0": "npm:effector@22.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "flowgen": "1.20.1",
    "np": "^7.6.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "patronum": "1.1.0",
    "prettier": "^2.7.1",
    "react-native-encrypted-storage": "^4.0.2",
    "rewiremock": "^3.14.3",
    "rollup": "^2.77.2",
    "rollup-plugin-command": "^1.1.3",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "size-limit": "^8.0.0",
    "snoop": "^1.0.4",
    "ts-node": "^10.9.1",
    "tsd": "^0.22.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "uvu": "0.5.4",
    "yaspeller": "^8.0.1"
  }
}
