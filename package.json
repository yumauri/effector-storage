{
  "name": "effector-storage",
  "description": "Module for Effector to sync stores with different storages",
  "version": "6.0.1",
  "author": "Victor Didenko <yumaa.verdin@gmail.com> (https://yumaa.name)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yumauri/effector-storage"
  },
  "bugs": {
    "url": "https://github.com/yumauri/effector-storage/issues"
  },
  "homepage": "https://github.com/yumauri/effector-storage#readme",
  "keywords": [
    "effector",
    "persist",
    "storage",
    "localstorage",
    "local storage",
    "sessionstorage",
    "session storage",
    "react native asyncstorage",
    "react native async storage",
    "react native encryptedstorage",
    "react native encrypted storage"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup --config",
    "build:dev": "rollup --config",
    "check:lint": "eslint src --ext .ts",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check src",
    "check:spell": "yaspeller .",
    "lint": "npm-run-all --parallel check:*",
    "format": "prettier --write src tests",
    "test": "npm-run-all --parallel test:*",
    "test:node": "c8 --src=src --all --reporter=lcov --reporter=text uvu -r ts-node/register -r tests/rewiremock -i mock -i types tests",
    "test:types": "uvu -r ts-node/register -r tests/rewiremock tests types",
    "test-one": "uvu -r ts-node/register -r tests/rewiremock -i mock -i types tests",
    "e2e": "playwright test",
    "e2e:server": "vite build --force -c e2e/empty-app/vite.config.ts && vite preview -c e2e/empty-app/vite.config.ts",
    "doc:update-readme": "npx doctoc --maxlevel 3 README.md",
    "clean": "rm -rf build coverage",
    "size": "size-limit --highlight-less"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-block-scoping": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@farfetched/core": "^0.8.13",
    "@farfetched/runtypes": "^0.8.13",
    "@playwright/test": "^1.35.1",
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@sinonjs/fake-timers": "^10.3.0",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/node": "^18.16.18",
    "@types/sinonjs__fake-timers": "^8.1.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "c8": "^8.0.0",
    "effector": "22.0.0",
    "effector-22-0-0": "npm:effector@22.0.0",
    "effector-22-8-6": "npm:effector@22.8.6",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "flowgen": "^1.21.0",
    "npm-run-all": "^4.1.5",
    "patronum": "^1.18.0",
    "prettier": "^2.8.8",
    "react-native-encrypted-storage": "^4.0.3",
    "rewiremock": "^3.14.5",
    "rollup": "^3.25.2",
    "rollup-plugin-command": "^1.1.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "runtypes": "^6.7.0",
    "size-limit": "^8.2.6",
    "snoop": "^1.0.4",
    "ts-node": "^10.9.1",
    "tsd": "^0.28.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "uvu": "0.5.6",
    "vite": "^4.3.9",
    "yaspeller": "^8.0.1"
  },
  "packageManager": "pnpm@8.6.4",
  "pnpm": {
    "patchedDependencies": {
      "uvu@0.5.6": "patches/uvu@0.5.6.patch"
    }
  }
}
